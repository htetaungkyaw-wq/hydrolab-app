# Public Supabase project URL used by the client and middleware
NEXT_PUBLIC_SUPABASE_URL=

# Public anonymous key for Supabase client-side access
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Service role key for server-side tasks that require elevated privileges
SUPABASE_SERVICE_ROLE_KEY=

# Optional: set to "true" locally (never in production) to bypass auth for admin/portal
DEV_BYPASS_AUTH=
